{
  "name": "svg-to-swiftui-converter-app",
  "description": "Web app to convert SVG files into SwiftUI Shape struct.",
  "engines": {
    "node": ">12"
  },
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write ./src/**/*.{svelte,ts,js} ./*.{ts,js}"
  },
  "devDependencies": {
    "@quassum/prettier-config": "^1.0.2",
    "@sveltejs/adapter-static": "^1.0.0-next.0",
    "@sveltejs/adapter-vercel": "^1.0.0-next.0",
    "@sveltejs/kit": "^1.0.0-next.1",
    "@tsconfig/svelte": "^3.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.10.2",
    "tailwindcss": "^3.0.19",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "svg-to-swiftui-core": "^0.2.7"
  }
}
